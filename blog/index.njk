---
layout: layout.njk
---

<h1>My blog</h1>

<ul>
{% for post in collections.blog %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ul>

<ul>
{% for article in news.articles %}
  <li>
    <a href="{{ article.url }}">{{ article.title }}</a>
    <span>{{ article.source.name }} - <time>{{ article.publishedAt }}</time></span>
  </li>
{% endfor %}
</ul>